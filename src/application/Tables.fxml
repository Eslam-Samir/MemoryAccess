<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.TablesController">
   <children>
      <Label fx:id="StageNumber">
         <font>
            <Font size="22.0" />
         </font>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <TableView fx:id="table">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="111.0" text="Process Name" />
          <TableColumn fx:id="sizeColumn" prefWidth="117.0" text="size " />
            <TableColumn fx:id="baseAddressColumn" prefWidth="112.0" text="Base Address" />
        </columns>
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </TableView>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button alignment="CENTER" contentDisplay="CENTER" minWidth="150.0" mnemonicParsing="false" onAction="#pressPrevious" text="Prevoius Stage">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="15.0" left="25.0" right="25.0" top="15.0" />
               </padding>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="150.0" mnemonicParsing="false" onAction="#pressNext" text="Next Stage">
               <font>
                  <Font size="14.0" />
               </font>
               <padding>
                  <Insets bottom="15.0" left="25.0" right="25.0" top="15.0" />
               </padding>
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
